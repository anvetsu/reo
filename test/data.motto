users:tab(['id 'name]
          [til(10)
           ["Hero" "Dunn" "Sue" "Chi"
            "Thor" "Clive" "Hicks" "Devin"
            "Kate" "Klein"]])
friendships:[[0 1] [0 2] [1 2] [1 3] [2 3] [3 4] [4 5] [5 6] [5 7] [6 8] [7 8] [8 9]]

cntfs:fn(i) sum(map(fn(x) if { fst(x) = i | snd(x) = i 1 0}, friendships))
numfs:fn(n) { i:pos(users('name) n) if { i >= 0 cntfs(i) 0}}
